<%= title [l(:label_print_templates_plural), print_templates_url], l(:label_print_templates_new) %>

<%= labelled_form_for @print_template, url: print_templates_path do |f| %>
  <%= render partial: 'form', locals: { f: f } %>
  <p>
    <%= submit_tag l :button_create %>
    <%= submit_tag l(:button_create_and_continue), name: 'continue' %>
  </p>
<% end %>
